template<class X,class Y>bool maximize (X &x,const Y &y){if(x < y){x = y;return 1;}return 0;}
template<class X,class Y> bool minimize (X &x,const Y &y){if(x > y){x = y;return 1;}return 0;}
namespace Love{
pair<int,int> a[21];
int ans = 0,n,W;
void btk(int i,int s,int w){
    //cout << i << ' ' << s << ' ' << w << '\n';
    FOR(ii,i + 1,n){
        if(w + a[ii].F <= W) btk(ii,s + a[ii].S,w + a[ii].F);
    }
    ans = max(ans,s);
}
void solve(){
    cin >> n >> W;
    FOR(i,0,n) cin >> a[i].F >> a[i].S;
    FOR(i,0,n) if(a[i].F <= W) btk(i,a[i].S,a[i].F);
    cout << ans << '\n';
}
}
signed main(){
//    freopen("bcount.in","r",stdin);
//    freopen("bcount.out","w",stdout);
#ifdef File
    freopen(FileName(File)".inp","r",stdin);
    freopen(FileName(File)".out","w",stdout);
#endif
    I LOVE GB;
    int t = 1;//cin >> t;
    while(t--)
    Love :: solve();
    return 0;
}
