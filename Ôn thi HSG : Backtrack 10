#define FOR(i,a,b) for(int i = (a);i < (b);i++)
#define REP(i,a,b) for(int i = (a);i <= (b);i++)
#define REPD(i,a,b) for(int i = (a); i >= (b);i--)
#define FORD(i,a,b) for(int i = (a);i > (b);i--)
#define F first
#define S second
#define pb push_back
#define BIT(mask,i) (mask) & (1 << (i))
#define CHECK(mask,i) (((mask) >> (i)) & 1)
