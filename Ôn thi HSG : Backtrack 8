template<class X,class Y>bool maximize (X &x,const Y &y){if(x < y){x = y;return 1;}return 0;}
template<class X,class Y> bool minimize (X &x,const Y &y){if(x > y){x = y;return 1;}return 0;}
namespace Love{
const int MAX = 11;
int vis[MAX][MAX];
int n,cnt = 0;
void backtrack(int i = 0,int j = 0){
    int ii = i - 1,jj = j - 1;
    while(ii >= 0 && jj >= 0){if(vis[ii][jj]) return; ii--;jj--;}
    ii = i - 1;jj = j + 1;
    while(ii >= 0 && jj < n) {if(vis[ii][jj]) return; ii--;jj++;}
    REPD(jjj,j - 1,0) if(vis[i][jjj]) return;
    REPD(iii,i - 1,0) if(vis[iii][j]) return;
    if(i == n - 1){
        cnt++;
//        FOR(iijjjj,0,n){FOR(jjjjj,0,n) cout << vis[iijjjj][jjjjj] << ' '; cout << '\n';}
//        cout << '\n';
        return;
    }
    vis[i][j] = 1;
    FOR(iiii,0,n){
        backtrack(i + 1,iiii);
    }
    vis[i][j] = 0;
}
void solve(){
    cin >> n;
    FOR(ii,0,n)backtrack(0,ii);
    cout << cnt << '\n';
}
}
signed main(){
//    freopen("bcount.in","r",stdin);
//    freopen("bcount.out","w",stdout);
#ifdef File
    freopen(FileName(File)".inp","r",stdin);
    freopen(FileName(File)".out","w",stdout);
#endif
    I LOVE GB;
    int t = 1;//cin >> t;
    while(t--)
    Love :: solve();
    return 0;
}
