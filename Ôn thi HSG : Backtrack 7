template<class X,class Y>bool maximize (X &x,const Y &y){if(x < y){x = y;return 1;}return 0;}
template<class X,class Y> bool minimize (X &x,const Y &y){if(x > y){x = y;return 1;}return 0;}
namespace Love{
const int MAX = 8;
int n,m;
bool check(int i,int j,vector<vector<int>> &vis){
    return i < n && i >= 0 && j < m && j >= 0 && vis[i][j] == 0;
}
const int dx[] = { 2, 2, 1, 1, -1, -1, -2, -2 };
const int dy[] = { 1,-1, 2,-2,  2, -2,  1, -1 };

void backtrack(int i,int j,vector<vector<int>> vis,int num = 0){
    vis[i][j] = num + 1;
    if(num + 1 == n * m){
        FOR(ii,0,n) {FOR(jj,0,m){
            cout << vis[ii][jj] << ' ';
        }
        cout << "\n\n";}
        exit(0);
    }
    FOR(d,0,8){
        int x = i + dx[d];
        int y = j + dy[d];
        if(check(x,y,vis)) backtrack(x,y,vis,num + 1);
    }
}
void solve(){
    cin >> n >> m;
    vector<vector<int>> vis(n + 1,vector<int> (m + 1));
    backtrack(0,0,vis);
}
}
signed main(){
//    freopen("bcount.in","r",stdin);
//    freopen("bcount.out","w",stdout);
#ifdef File
    freopen(FileName(File)".inp","r",stdin);
    freopen(FileName(File)".out","w",stdout);
#endif
    I LOVE GB;
    int t = 1;//cin >> t;
    while(t--)
    Love :: solve();
    return 0;
}
