YÃªu cáº§u: Cho hai sá»‘ nguyÃªn n vÃ  k, hÃ£y xÃ¢y dá»±ng má»™t táº­p A gá»“m ğ‘˜ sá»‘ tá»± nhiÃªn khÃ¡c nhau cÃ³ tá»•ng 
báº±ng ğ‘› vÃ  lÃ  má»™t bá»™ sinh bÃ¬nh phÆ°Æ¡ng, hoáº·c cho biáº¿t ráº±ng khÃ´ng tá»“n táº¡i táº­p há»£p nhÆ° váº­y? 


#include<bits/stdc++.h>
using namespace std;
#define int long long
signed main(){
    freopen("SQUARE.INP","r",stdin);
    freopen("SQUARE.OUT","w",stdout);
    ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    int n,k;cin >> n >> k;
    vector<int> x(k);
    bool ok = 0;
    for(int b = 1;b <= 2000;b++){\
        int s = 0;
        for(int i = 0;i < k;i++){
            x[i] = b + i;
            s += x[i] * x[i];
        }
        if(s == (k - 1) * n){
            ok = 1;
            break;
        }
    }
    if(!ok) {
        cout << "NO\n";
        return 0;
    }
    vector<int> a(k);
    for(int i = 0;i < k;i++){
        a[i] = n - x[i] * x[i];
    }
    set<int> st(a.begin(),a.end());
    if(st.size() != k){cout <<"NO\n";return 0; }
    cout << "YES\n";
    for(int &i : a) cout << i << ' ';
}
