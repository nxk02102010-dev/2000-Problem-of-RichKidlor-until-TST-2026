namespace Love{
void backtrack(int n,int k,vector<int> &a,int group,vector<int> &chosen){
    if(sz(chosen) == n){
        vector<int> mp(k + 1);
        FOR(i,0,n){
            mp[chosen[i]] += a[i];
        }
        int t = mp[1];
        REP(i,1,k) if(t != mp[i]) return;
        for(int &i : chosen) cout << i << ' ';
        exit(0);
    }
    REP(i,1,k){
        chosen.pb(i);
        backtrack(n,k,a,i,chosen);
        chosen.pop_back();
    }
}
void solve(){
    int n,k;cin >> n >> k;
    vector<int> a(n + 1);
    REP(i,0,n - 1) cin >> a[i];
    REP(i,1,k){vector<int> chosen ={i}; backtrack(n,k,a,i,chosen);}
    cout << "ze" << '\n';
}
}
signed main(){
//    freopen("bcount.in","r",stdin);
