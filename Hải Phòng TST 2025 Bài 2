chá»n cÃ¡ch Ä‘i tá»‘n Ã­t nhiÃªn liá»‡u nháº¥t.


#include<bits/stdc++.h>
using namespace std;
#define int long long
#define F first
#define S second
#define FOR(i,a,b) for(int i = (a);i < (b);i++)
const int MAXN = 300100,MAXE = 300100;
struct E1{
    int v,p,w;
};
struct E2{
    int u,v,p,w;
    bool operator<(const E2&other) const{
        return p < other.p;
    }
};
vector<E1> g[MAXN];
vector<E2> e;
int n,m,p[MAXN];
int find(int v) {  return v == p[v] ? v : p[v] = find(p[v]);  }
void unite(int a,int b){
    a = find(a);
    b = find(b);
    if(a == b) return;
    p[b] = a;
    return;
}
void buildGraph(){
    cin >> n >> m;
    int a,b,c,d;
    FOR(i,0,m){
        cin >> a >> b >> c >> d;
        g[a].push_back({b,c,d});
        g[b].push_back({a,c,d});
        e.push_back({a,b,c,d});
    }
    iota(p,p + n + 1,0);
}
void solve(void){
    int mx;
    sort(e.begin(),e.end());
    FOR(i,0,e.size()){
        unite(e[i].u,e[i].v);
        if(find(1) == find(n)){
            mx = e[i].p;
            break;
        }
    }
    vector<int> dis(n + 1,4e18);
    priority_queue<pair<int,int>,vector<pair<int,int>>,greater<pair<int,int>>> q;
    q.push({0,1});
    dis[1] = 0;
    while(!q.empty()){
        pair<int,int> top = q.top();q.pop();
        int u = top.S;
        int wu = top.F;
        if(dis[u] != wu) continue;
        for(auto t : g[u]){
            int v = t.v;
            if(t.p > mx) continue;
            if(dis[v] > wu + t.w){
                dis[v] = wu + t.w;
                q.push({dis[v],v});
            }
        }
    }
    cout << mx << ' ' << dis[n] << '\n';
}
signed main(){
    ios::sync_with_stdio(0);cin.tie(0);cout.tie(0);
    buildGraph();
    solve();
}
