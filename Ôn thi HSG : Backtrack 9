#define ll long long
template<class X,class Y>bool maximize (X &x,const Y &y){if(x < y){x = y;return 1;}return 0;}
template<class X,class Y> bool minimize (X &x,const Y &y){if(x > y){x = y;return 1;}return 0;}
namespace Love{
    int a[5][5];
    bool check(int i,int j){return i < 4 && j < 4 && i >= 0 && j >= 0;}
    int ans = 0;
    void btk(int i = 0,int j = 0,int s = a[0][0]){
        if(i == 3 && j == 3) {ans = max(ans,s);return;}
        if(check(i + 1,j)) btk(i + 1,j,s + a[i + 1][j]);
        if(check(i,j + 1)) btk(i,j + 1,s + a[i][j + 1]);
    }
void solve(){
    FOR(i,0,4) FOR(j,0,4) cin >> a[i][j];
    btk();
    cout << ans << '\n';
}
}
signed main(){
//    freopen("bcount.in","r",stdin);
//    freopen("bcount.out","w",stdout);
#ifdef File
    freopen(FileName(File)".inp","r",stdin);
    freopen(FileName(File)".out","w",stdout);
#endif
    I LOVE GB;
    int t = 1;//cin >> t;
    while(t--)
    Love :: solve();
    return 0;
}
